#include <stdio.h>
#include <stdlib.h>
#include <unistd.h>
#include <ctype.h>
#include <sys/wait.h>

int main(void) {
	pid_t pid;

	if((pid = fork())==-1)
	{
		perror("fork");
		return EXIT_FAILURE;
	}

	if(pid ==0)
	{
		printf("Witam w kliencie!\n");
	}
	else
	{
		printf("Witam w hoscie!\n PID: %d\n",pid);
	}
	return EXIT_SUCCESS;
}
