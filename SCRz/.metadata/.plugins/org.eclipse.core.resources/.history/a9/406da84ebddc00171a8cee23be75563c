#include <stdio.h>
#include <stdlib.h>
#include <signal.h>

int main(int argc, char* argv[]) {
	printf("%d, %d, %d", argv[1],argv[2],argv[3]);
	int pid = (int)argv[2];
	int signal=(int)argv[1];
	union sigval value;

	value.sival_int = (int)argv[3];

	sigqueue(pid,SIGRTMIN,value); //wysylanie sygnalu

	return EXIT_SUCCESS;
}
